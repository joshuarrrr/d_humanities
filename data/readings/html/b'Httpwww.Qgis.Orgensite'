<script type="text/javascript">
    $(function(){
        // create/start carousel
        $('.carousel').carousel();

        // load atomfeeds
        atom('/feeds/qugsnews.atom', '#qugsatom', 5, true);
        atom('/feeds/qgisplanet.atom', '#qgisplanetatom', 5, true);
        atom('/feeds/qgisflickrmaps.atom', '#qgisflickrmapsatom', 5, false);

        $('[data-countdown]').each(function() {
          var $this = $(this);
          var finalDate = $(this).data('countdown');
          if( finalDate != "None" ) {
                $this.countdown(finalDate)
                        .on("update.countdown", function(event) {
                                $this.html( finalDate + " " + event.strftime('%-Dd %-Hh %-Mm'));
                        })
                        .on("finish.countdown", function(event) {
                                if( $this.parent().attr('id') == 'nextfeaturefreeze' ) {
                                        $this.parent().html( "2.99 in feature freeze since " + event.finalDate.toLocaleString() + "<br/>" );
                                } else if( $this.parent().attr('id') == 'nextrelease' ) {
                                        $('#nextfeaturefreeze').html("");
                                        $('#nextrelease').html( "3.0 in packaging since " + event.finalDate.toLocaleString() + "<br/>" );
                                } else if( $this.parent().attr('id') == 'nextpointreleasedate' ) {
                                        $('#nextpointreleasedate').html("");
                                }
                        });
          } else {
                $( '#' + $this.parent().attr('id') ).html( "" );
          }
        });
    });
</script>
    

